{
  "name": "node_boilerplate",
  "version": "1.0.0",
  "description": "node_boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=production node ./bin/www",
    "dev": "nodemon ./bin/www",
    "make:migration": "node ./node_modules/sequelize-auto-migrations/bin/makemigration --name test",
    "db:init": "npx sequelize init ",
    "db:init:migrations": "npx sequelize init ",
    "db:init:seeders": "npx sequelize init ",
    "db:create": "npx sequelize db:create ",
    "db:migrate": "npx sequelize db:migrate",
    "db:migration:generate": "npx sequelize migration:generate --name test",
    "db:migrations:add:timestamp": " sequelize db:migrate:schema:timestamps:add",
    "db:migrateundo": "npx sequelize db:migrate:undo:all ",
    "db:seed": "npx sequelize db:seed:all ",
    "db:seedundo": "npx sequelize db:seed:undo:all ",
    "db:drop": "npx sequelize db:drop",
    "db:model:create": "npx sequelize model:generate "
  },
  "author": "qwerty Systems <qwerty.co.ke>",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^6.12.0",
    "app-root-path": "^3.0.0",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "geoip-lite": "^1.4.2",
    "helmet": "^4.6.0",
    "http-status": "^1.5.2",
    "i": "^0.3.7",
    "joi": "^17.4.0",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.4.17",
    "npm": "^7.6.0",
    "pg": "^8.7.1",
    "randomstring": "^1.1.5",
    "rate-limiter-flexible": "^2.2.1",
    "sequelize": "^5.22.4",
    "sequelize-auto-migrations": "^1.0.3",
    "sequelize-cli": "^5.5.1",
    "short-uuid": "^4.2.0",
    "socket.io": "^4.5.1",
    "url": "^0.11.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "winston": "^3.2.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.14"
  }
}
